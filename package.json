{
  "name": "labsync",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "check": "biome check --write . --max-diagnostics 400",
    "dev": "turbo dev",
    "build": "turbo build",
    "check-types": "turbo check-types",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "dev:server": "turbo -F @labsync/backend dev",
    "dev:setup": "turbo -F @labsync/backend dev:setup",
    "test": "turbo test:once",
    "test:watch": "turbo test",
    "test:coverage": "turbo test:coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "turbo": "^2.6.0",
    "ultracite": "5.4.7"
  },
  "packageManager": "pnpm@10.20.0"
}
